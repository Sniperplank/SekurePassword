{
    "name": "Sekure Password",
    "description": "Store and manage your passwords. Sign into sites quicker with the autofill feature.",
    "version": "1.2.2",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16": "lock-icon-16px.png",
            "32": "lock-icon-32px.png",
            "48": "lock-icon-48px.png",
            "128": "lock-icon-128px.png"
        }
    },
    "permissions": [
        "tabs",
        "storage",
        "scripting",
        "activeTab"
    ],
    "icons": {
        "16": "lock-icon-16px.png",
        "32": "lock-icon-32px.png",
        "48": "lock-icon-48px.png",
        "128": "lock-icon-128px.png"
    }
}