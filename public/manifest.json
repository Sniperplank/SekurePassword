{
    "name": "Sekure Password",
    "description": "Manage passwords online",
    "version": "1.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "index.html"
    },
    "permissions": [
        "https://sekure-password-server.vercel.app/*",
        "contextMenus",
        "tabs",
        "webNavigation",
        "activeTab",
        "alarms",
        "clipboardWrite",
        "storage",
        "scripting"
    ]
}