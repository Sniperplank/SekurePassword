{
    "name": "Sekure Password",
    "description": "Manage passwords online",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "mainPopUp.html"
    },
    "permissions": [
        "contextMenus",
        "tabs",
        "alarms",
        "clipboardWrite"
    ]
}